#include <iostream>  
using namespace std;  

void swap(int &a ,int &b){
	int temp =a;
	a = b;
	b= temp;
}
void bubble_sort(int a[],int len)  
{  
	for(int i=0;i<len-1;i++)
	{
		for(int j=0;j<len-i-1;j++)
		{
			if(a[j]>a[j+1])
				swap(a[j],a[j+1]);
		}

	} 
     
}  
int main()  
{  
    int a[] = {100,8,15 ,37,26,13,27,49,55,14};
    cout<<"Before sort: ";  
    for(int i =0 ; i<10;i++)  
        cout<<a[i]<<" ";  
    cout<<endl;
    int len = sizeof(a)/sizeof(a[0]);
    bubble_sort(a,len); 
    cout<<"After sort: ";  
    for(int i =0 ; i<10;i++)  
        cout<<a[i]<<" ";  
    cout<<endl;  
}

 
